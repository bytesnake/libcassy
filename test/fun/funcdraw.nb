Cell[CellGroupData[{

Cell["\<\
Observe how the interpolated wave looks repeated three times (watch for \
discontinuities)\
\>", "Section",
 CellChangeTimes->{{3.577913087352275*^9, 3.5779131057443266`*^9}, {
  3.5779134057534866`*^9, 3.577913410048732*^9}}],

Cell[BoxData[
 DynamicBox[ToBoxes[
   Plot[
    Evaluate[
     Interpolation[
      DeleteDuplicates[$CellContext`coords, Part[#, 1] == Part[#2, 1]& ]][
      Mod[$CellContext`x, 1.]]], {$CellContext`x, 0, 3}, 
    PlotRange -> {{0, 3}, {-1, 1}}, AspectRatio -> 1/5, 
    ImageSize -> {1200, 240}], StandardForm],
  ImageSizeCache->{1200., {118., 122.}}]], "Output",
 CellChangeTimes->{
  3.5779121686777296`*^9, {3.5779124360360217`*^9, 3.5779125284613085`*^9}}]
}, Open  ]],

Cell[CellGroupData[{

Cell["\<\
Save the wave data (8000 samples) into a simple line-by-line format\
\>", "Section",
 CellChangeTimes->{{3.57791311892008*^9, 3.5779131387362137`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"data", "=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"Evaluate", "[", 
      RowBox[{
       RowBox[{"Interpolation", "[", 
        RowBox[{"DeleteDuplicates", "[", 
         RowBox[{"coords", ",", 
          RowBox[{
           RowBox[{
            RowBox[{"#1", "[", 
             RowBox[{"[", "1", "]"}], "]"}], "\[Equal]", 
            RowBox[{"#2", "[", 
             RowBox[{"[", "1", "]"}], "]"}]}], "&"}]}], "]"}], "]"}], "[", 
       FractionBox["k", "8000"], "]"}], "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"k", ",", "0", ",", "7999"}], "}"}]}], "]"}]}], ";", 
  RowBox[{"Export", "[", 
   RowBox[{"\"\<output.dat\>\"", ",", 
    RowBox[{"IntegerPart", "[", 
     RowBox[{
      FractionBox["2048", 
       RowBox[{"Max", "[", "data", "]"}]], "data"}], "]"}]}], 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.5779122775979595`*^9, 3.5779123179452677`*^9}, {
  3.577912642696842*^9, 3.5779127866580763`*^9}}],

Cell[BoxData[
 RowBox[{"SystemOpen", "[", 
